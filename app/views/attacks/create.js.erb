var point = $(".enemy-board div[data-row='<%= @coor_x %>'] .point[data-col='<%= @coor_y%>']");

console.log("Attacked <%= @coor_x %> <%= @coor_y %>")
console.log("Board <%= @attack.user.board.id %> ")
<% if @attack.user.board.attacked?(@coor_x, @coor_y) %>
